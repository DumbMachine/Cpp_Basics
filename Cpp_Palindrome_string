#include <iostream>
using namespace std;

int main() {
  string s="aaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaabaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaa";
  int i=0;
  int flag=1;
  int j=s.size()-1;
  while(i<=j)
  {
    if(s[i]!=s[j]) flag=0;

    i++;
    j--;
  }
  if(flag==1){cout<<"This is Palindrome\n";}
  else{cout<<"This is not a Palindrome\n";}
}
